{
    "name": "Foodino",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "start": "react-native start",
        "build": "babel src -d lib",
        "test": "jest",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "build-and": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd ../",
        "build-ios": "react-native run-ios --configuration Release",
        "bundle-and": "cd android && ./gradlew clean && ./gradlew bundleRelease && cd ../",
        "prettier-fix": "prettier --write **/*.js",
        "prettier-check": "prettier --list-different **/*.js",
        "lint-fix": "eslint --fix .",
        "lint-check": "eslint .",
        "rename": "react-native-rename",
        "postinstall": "patch-package && npx jetify",
        "clean": "cd android && ./gradlew clean && ./gradlew cleanBuildcache && cd .. && cd ios && rm -rf build/ && xcodebuild clean && watchman watch-del-all && cd ..",
        "pod": "cd ios && pod install && cd ..",
        "pod-m1": "cd ios && sudo arch -x86_64 gem install ffi && arch -x86_64 pod install && cd ..",
        "pod-update": "pod --repo-update",
        "pod-update-m1": "cd ios && sudo arch -x86_64 gem install ffi && arch -x86_64 pod install && arch -x86_64 pod update",
        "clean:ios": "cd ios && xcodebuild clean && cd ../",
        "clean:android": "cd android && ./gradlew clean && cd ../",
        "generate:ts": "graphql-codegen --config codegen.yml",
        "adb": "adb reverse tcp:8081 tcp:8081"
    },
    "dependencies": {
        "@react-native-async-storage/async-storage": "^1.17.11",
        "@react-navigation/bottom-tabs": "^6.4.0",
        "@react-navigation/native": "^6.0.13",
        "@react-navigation/native-stack": "^6.9.1",
        "@tanstack/react-query": "^4.15.1",
        "@types/react-native-vector-icons": "^6.4.12",
        "axios": "0.27.2",
        "eslint-plugin-prettier": "^4.2.1",
        "lottie-react-native": "^5.1.4",
        "native-base": "^3.4.21",
        "react": "18.1.0",
        "react-native": "0.70.5",
        "react-native-fast-image": "8.5.11",
        "react-native-safe-area-context": "^4.4.1",
        "react-native-screens": "^3.18.2",
        "react-native-svg": "12.1.1",
        "react-native-vector-icons": "^9.2.0",
        "yup": "^0.32.11",
        "zustand": "^4.1.4"
    },
    "devDependencies": {
        "@babel/core": "^7.12.9",
        "@babel/runtime": "^7.12.5",
        "@react-native-community/eslint-config": "^2.0.0",
        "@tsconfig/react-native": "^2.0.2",
        "@types/jest": "^26.0.23",
        "@types/react": "^18.0.21",
        "@types/react-native": "^0.70.6",
        "@types/react-test-renderer": "^18.0.0",
        "@typescript-eslint/eslint-plugin": "^5.37.0",
        "@typescript-eslint/parser": "^5.37.0",
        "babel-jest": "^26.6.3",
        "babel-plugin-module-resolver": "^4.1.0",
        "eslint": "^8.27.0",
        "jest": "^26.6.3",
        "metro-react-native-babel-preset": "0.72.3",
        "prettier": "2.7.1",
        "react-test-renderer": "18.1.0",
        "typescript": "^4.8.3"
    },
    "jest": {
        "preset": "react-native",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    }
}
